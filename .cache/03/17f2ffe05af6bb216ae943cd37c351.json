ver down */\r\n.with-cover-enter .transition {\r\n     transform: translate(0%);\r\n}\r\n\r\n.with-cover-enter-to .transition {\r\n     transform: translateX(100%);\r\n}","css\\app.scss":"\r\n// @import '~reset-css/reset.css';\r\n@import 'home.scss';\r\n@import 'fade.scss';\r\n@import 'clip.scss';\r\n@import 'with-cover.scss';\r\n\r\n:root {\r\n  --easing: cubic-bezier(0.5, 0.7, 0.4, 1);\r\n}\r\n\r\nbody {\r\n  --page-background: #fff;\r\n\r\n  font-family: 'Segoe UI';\r\n  font-weight: 300;\r\n  overflow: hidden;\r\n  background-color: var(--page-background);\r\n  margin: 0%;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6 {\r\n  font-family: 'Montserrat', sans-serif;\r\n  font-weight: 900;\r\n}\r\n\r\nheader {\r\n  --base-color: #fff;\r\n\r\n  width: calc(100% - 28vw);\r\n  height: 100vh;\r\n  color: var(--base-color);\r\n  display: flex;\r\n  align-content: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  padding: 0 14vw;\r\n\r\n  h1 {\r\n    font-size: 5vw;\r\n    margin-bottom: 2.5vh;\r\n    line-height: 1.4;\r\n  }\r\n\r\n  p {\r\n    font-size: 2.5vw;\r\n    line-height: 1.4;\r\n    opacity: 0.8;\r\n    margin-bottom: 5.375vh;\r\n  }\r\n\r\n  a {\r\n    font-size: 2.22vw;\r\n    padding: 24px 2vw;\r\n    display: inline-block;\r\n    text-align: center;\r\n    color: var(--base-color);\r\n    border: 3px solid  var(--base-color);\r\n    border-radius: 10px;\r\n    text-decoration: none;\r\n  }\r\n\r\n  &.is-home {\r\n    background-color: #5c85ee;\r\n  }\r\n\r\n  &.is-fade {\r\n    --base-color: #000;\r\n\r\n    background-color: #cc98c6;\r\n  }\r\n\r\n  &.is-clip {\r\n    background-color: #161636;\r\n  }\r\n\r\n  &.is-cover {\r\n    background-color: #e66f7f;\r\n  }\r\n}\r\n\r\n.content {\r\n  transition: all 0.3s ease-out;\r\n  opacity: 1;\r\n  transform: translateY(0);\r\n}\r\n\r\n.transition {\r\n  position: fixed;\r\n  width: 100%;\r\n  height: 100%;\r\n  top: 0;\r\n  left: 0;\r\n  background-color: #c8cc98;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  transform: translateY(-100%);\r\n  z-index: 1;\r\n\r\n  > h2 {\r\n    font-size: 40px;\r\n    color: #202020;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"01e4a1e0d1fed4dfb73d28dc0ca97516","cacheData":{"env":{}}}